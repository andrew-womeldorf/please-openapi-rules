[please]
Version = 16.20.0-beta.14

[Plugin "js"]
Target = //plugins:js

[PluginDefinition]
name = openapi

[PluginConfig "generator_tool"]
ConfigKey = GeneratorTool
DefaultValue = //third_party/java:openapi-generator-cli

[PluginConfig "openapi_tool"]
ConfigKey = OpenAPITool
DefaultValue = //third_party/js:openapi-cli

[PluginConfig "redoc_tool"]
ConfigKey = RedocTool
DefaultValue = //third_party/js:redoc-cli-bin

[PluginConfig "spectral_tool"]
ConfigKey = SpectralTool
DefaultValue = //third_party/js:spectral-cli

[PluginConfig "validate_tool"]
ConfigKey = ValidateTool
DefaultValue = //third_party/js:dredd-bin
